#include <iostream>
#include <string>
#include <sstream>

int main() 
{
    std:: string hexNumber; 
    std::cout << "Unesite heksadecimalni broj: ";
    std::cin >> hexNumber;

    unsigned int decimalNumber;
    std::stringstream ss;

    ss << std::hex << hexNumber;

    ss >> decimalNumber;

    std::cout << "Decimalni broj je: " << decimalNumber << std::endl;

    return 0;
}
